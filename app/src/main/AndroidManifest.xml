<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wolfmaker.fvstore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/f_vstore"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.wolfmaker.fvstore.grocery.organicfruitsActivity"
            android:label="organic fruits-Vegetables" />
        <activity
            android:name="com.wolfmaker.fvstore.grocery.FruitsActivity"
            android:label="Fruits-vegetables" />
        <activity
            android:name="com.wolfmaker.fvstore.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wolfmaker.fvstore.MainActivity"
            android:label="@string/f_vstore"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>